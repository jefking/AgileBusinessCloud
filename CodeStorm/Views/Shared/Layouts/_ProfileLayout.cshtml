<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="description" content="@ViewBag.Description">
  <meta name="author" content="ABC">
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
  <title>@ViewBag.Title</title>
  <meta property="og:type" content="profile" />
  <meta property="og:title" content="@ViewBag.Title" />
  <meta property="og:image" content="//gravatar.com/avatar/@ViewBag.Gravatar?s=150" />
  <meta property="og:description" content="@ViewBag.Description" />
  <meta property="og:url" content="@Request.Url.ToString()" />
  @Html.Partial("_Tracking")
  <link href='http://fonts.googleapis.com/css?family=Play:400,700' rel='stylesheet'
    type='text/css'>
  @Styles.Render("~/Content/css/CodeStorm")
  <script src="//cdnjs.cloudflare.com/ajax/libs/modernizr/2.6.2/modernizr.min.js"></script>
  <script data-main="@Url.Content("~/Scripts/codestorm/app.js")" src="@Url.Content("~/Scripts/codestorm/require-jquery.js")"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.1.1/bootstrap.min.js"></script>
  @Scripts.Render("~/bundles/CodeStormPlugins")
</head>
<body>
  @Html.Partial("_Header")
  @RenderBody()
  @Html.Partial("_Footer")
</body>
</html>
