@model Abc.Website.Models.UserPublicProfile

<p><i class="icon-user"></i>&nbsp;Joined on @Model.CreatedOn.ToString("dd MMM yyyy")</p>

@if (Model.City != null)
{
  <p><i class="icon-globe"></i>&nbsp;@Model.City</p>

}
@if (Model.TwitterHandle != null)
{
  <p>
    <a href="https://twitter.com/@Model.TwitterHandle" class="twitter-follow-button" data-show-count="false" data-size="large">Follow @@Model.TwitterHandle</a>
    <script>!function (d, s, id) { var js, fjs = d.getElementsByTagName(s)[0]; if (!d.getElementById(id)) { js = d.createElement(s); js.id = id; js.src = "//platform.twitter.com/widgets.js"; fjs.parentNode.insertBefore(js, fjs); } }(document, "script", "twitter-wjs");</script>

  </p>
}
@if (Model.GitHubHandle != null)
{
  <p><i class="icon-share"></i>&nbsp;<a href="http://github.com/@Model.GitHubHandle" target="_blank" title="Github">Github: @Model.GitHubHandle</a></p>
}

