@{
    ViewBag.OGraphImage = "http//gravatar.com/avatar/03e81268ddfe858d6aa8edec847e6e15";

    string tweet = string.Empty;
    if (null != this.Model.Post)
    {
        ViewBag.Title = string.Format("{0} | George Danes | ABC", this.Model.Post.Title);
        tweet = string.Format("{0} by @GeorgeDanesABC via @WeAreABC", this.Model.Post.Title);
    }
    else
    {
        ViewBag.Title = "George Danes's Blog | ABC";
        tweet = "Thoughts by George Danes via @GeorgeDanesABC @WeAreABC";
    }
}
<div class="line">
