    <div class="panel">
      <div class="title">
        <h4>George Danes > Chief Financial Officer</h4>
        <div class="theme">
        </div>
        <div class="drop">
        </div>
      </div>
      <div class="content inpad">
      </div>
    </div>
  </div>
  <div class="small">
    <div class="section full line">
      <ul>
      </ul>
    </div>
    <div class="section line full">
      @Html.Partial("_Social")
    </div>
  </div>
</div>
